{
    "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "outDir": "./dist",
    "target": "es6",
    "lib": ["ESNext", "dom", "dom.iterable"],
    "noImplicitAny": true, 
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    },
    "include": ["./src/*.ts"],
    "exclude": ["node_modules", "dist"]
}
